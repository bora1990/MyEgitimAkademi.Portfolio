
@model List<MyEgitimAkademi.Portfolio.Models.Service>

<section class="w3l-homeblock1" id="about">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6">
                    <span class="text" style="color:lightcoral">Hizmetlerim</span>
                    <h3 class="title-big"><span>Markanı inşa et</span> ve işini geliştir.</h3>
                    <p class="mt-4">
                        Size iş dünyası için yeni bir bakış açısı kazandırabilirim.Benim Cv'mi indirerek yazılımcı olarak profesyonel yaşamımın detaylarını görebilirsiniz.
                    </p>
                    <ul class="service-list mt-md-5 mt-4">
                        @foreach(var item in Model)
            {
                <li class="service-link"><a href="#">@item.ServiceName</a></li>

            }             
                    </ul>
                    <a href="#url" class="btn btn-style btn-empty p-0">Tüm Hizmetlerimi Görün...<span></span></a>
                </div>
                <div class="col-lg-6 mt-lg-0 mt-5">
                    <div class="position-relative">
                        <img src="~/Templates/iportfolio-starter/assets/images/about.jpg" alt="" class="radius-image img-fluid">
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>